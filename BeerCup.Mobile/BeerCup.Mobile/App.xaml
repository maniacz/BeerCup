<?xml version="1.0" encoding="utf-8" ?>
<Application xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:converters="clr-namespace:BeerCup.Mobile.Converters"
             x:Class="BeerCup.Mobile.App">
    <Application.Resources>
        <ResourceDictionary>
            <converters:ItemTappedConverter x:Key="LocalItemTappedConverter"></converters:ItemTappedConverter>
            <converters:BeerNoConverter x:Key="LocalBeerNoConverter"></converters:BeerNoConverter>
            <converters:UserVoteToColorConverter x:Key="LocalUserVoteToColorConverter"></converters:UserVoteToColorConverter>
        </ResourceDictionary>
    </Application.Resources>
</Application>